			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="T_Department" schema="dbo"
		catalog="FileManagementSystemDB" class="model.TDeparment">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="T_Authority" schema="dbo"
