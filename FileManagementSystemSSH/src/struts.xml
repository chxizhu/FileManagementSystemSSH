<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="default" extends="struts-default" namespace="/">
		<action name="loginAction" class="control.action.loginAction">
			<result name="success" type="dispatcher">/index.jsp</result>
			<result name="error" type="dispatcher">/login.jsp</result>
		</action>
		<action name="UserinformationEditAction"
			class="control.action.UserinformationEditAction">
			<result name="error">/eitd.jsp</result>
			<result name="success">/eitd.jsp</result>
		</action>
		<action name="loginout" class="control.action.LoginOutAction">
			<result name="success" type="dispatcher">/login.jsp</result>
		</action>
		<action name="seek" class="control.action.SeekAction">
			<result name="success" type="dispatcher">/seek.jsp</result>
			<result name="error" type="dispatcher">/error.jsp</result>
		</action>
		<action name="addfileAction"
			class="control.action.addfileAction">
			<result name="success" type="dispatcher">
				/filesucceed.jsp
			</result>
		</action>
		<action name="error" class="control.action.ErrorAction">
			<result name="success" type="dispatcher">/seek.jsp</result>
			<result name="error" type="dispatcher">/error.jsp</result>
		</action>
		<action name="ownfile" class="control.action.ownfileAction">
			
		</action>
		<action name="PassdEitsaction"
			class="control.action.PassdEitsaction">
			<result name="error">/login.jsp</result>
		</action>
		<action name="downloadAction"
			class="control.action.downloadAction">
			<result name="error">/error.jsp</result>
			<result name="success" type="dispatcher">
				/download.jsp
			</result>
		</action>
		<action name="deleteownfile"
			class="control.action.deleteownfileAction">
			<result name="success" type="dispatcher">
				/ownfile.jsp
			</result>
		</action>
		<action name="categoryseek"
			class="control.action.CategorySeekAction">
			<result name="success" type="dispatcher">/seek.jsp</result>
			<result name="error" type="dispatcher">/error.jsp</result>
		</action>
		<action name="seekimg" class="control.action.SeekImgAction">
			<result name="success" type="dispatcher">
				/seekImg.jsp
			</result>
			<result name="error" type="dispatcher">/error.jsp</result>
		</action>
		<action name="fileupdating"
			class="control.action.fileupdatingAction">
			<result name="success" type="dispatcher">
				/fileupdating.jsp
			</result>
		</action>
		<action name="returnindex"
			class="control.action.ReturnIndexAction">
			<result name="success" type="dispatcher">/index.jsp</result>
		</action></package></struts>    
